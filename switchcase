#include<stdio.h>
main()
{
	printf("pick ur num - Pizza\n burger\n  Pasta\n  French Fries\n Sandwich\n ");
	int choice ;
	scanf("%d",&choice);
	switch(choice)
	{
	  	case 1: printf(" Food item-Pizza \n price- Rs 239");
	  	break;
		case 2: printf("Food item-Burger \n price- Rs 129");
		break;
		case 3: printf("Food item-Pasta \n price- Rs 179"); 
		break;
		case 4: printf("Food item-French  \n price- Rs 99");
		break;
		case 5: printf("Food item-sandwich \n price- Rs 149");
		break;
		default: printf("Food item-invalid ");
	}
}
